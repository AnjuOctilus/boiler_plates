{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework. Version : 0.0.6",
    "version": "0.0.7",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "elasticsearch/elasticsearch": "^7.12",
        "illuminate/redis": "^8.37",
        "laravel/lumen-framework": "^8.0",
        "laravelista/lumen-vendor-publish": "^8.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "monolog/monolog": "^2.2",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.4",
        "webpatser/laravel-uuid": "^4.0",
        "aws/aws-sdk-php": "^3.135",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "sendgrid/sendgrid": "^7.3",
        "elasticsearch/elasticsearch": "^7.14",
        "shiftonelabs/laravel-sqs-fifo-queue": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "exclude-from-classmap": [
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }

}
